clc;
clear;
close;
count=csvRead("C:\Users\Admin\Downloads\count.dat");
for i= 1:3
    scf();
    mu(i)= mean(count(:,i));
    sigma(i)= stdev(count(:,i));
    histplot(24,count(:,i));
    xtitle('Histogram of traffic intersection'+string(i));
end
MeanTotal = mean(mean(count));
disp('Mean for indivisual traffic intersection is =',mu);
disp('standerd deviation for indivisual traffic intersection is = ',sigma);
    disp(mu, "Mean for individual traffic intersection is =");
disp(sigma, "Standard deviation for individual traffic intersection is =");
disp(MeanTotal, "Overall mean =");

